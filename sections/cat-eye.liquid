<div class="lash-guide-template e-series">
<div class="page-header">
  <div class="row">
    <div class="col-sm-12">
    {{ section.settings.title }}
    </div>
  </div>
</div>
  
  
<div class="page-wrapper">
{% for block in section.blocks %}  
<div class="row lush_guide_row">
  
  <div class="col-sm-2">
    <div class="product-title">
      <a href="{{ block.settings.product_url }}">
      <p>{{ block.settings.product_title }}</p>
      <span>{{ block.settings.product_size }}</span>
      </a>
    </div>
  </div>
  
  <div class="col-sm-3">
    <div class="first-image">
      <img src="{{ block.settings.product_first | img_url:'170x' }}" class="product-image">
    </div>
  </div>
  
  <div class="col-sm-3">
    <div class="second-class">
     <img src="{{ block.settings.product_second | img_url:'170x' }}" class="product-image">
    </div>
  </div>
  
  <div class="col-sm-4">
    <div class="product-content">
      <a href="{{ block.settings.product_url }}">
      {{ block.settings.product_content }}
      </a>
    </div>
  </div>
  
  
  
  
  
  
</div>
{% endfor %}  
</div>
</div>




{% schema %}
{
  "name": "Cat Eye",
  "class": "index-section",
  "max_blocks": 11,	
  "settings": [
	{
        "type": "html",
        "id": "title",
        "label": "Title"
    }
  ],
  "blocks": [
	{
      "type": "text",
      "name": "text",
      "settings": [
      {
        "type": "text",
        "id": "product_title",
        "label": "Product Title"
      },
	  {
        "type": "text",
        "id": "product_size",
        "label": "Product size"
      },
	  {
        "type": "image_picker",
        "id": "product_first",
        "label": "Product size"
      },
	  {
        "type": "image_picker",
        "id": "product_second",
        "label": "Product size"
      },
	  {
        "type": "richtext",
        "id": "product_content",
        "label": "Product content"
      },
	  {
        "type": "url",
        "id": "product_url",
        "label": "Product link"
      }
	 ]
	}
  ],
  "presets": [
    {
      "name": "Cat Eye",
      "category": "custom"
    }
  ]
}
{% endschema %}
